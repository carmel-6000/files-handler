{
  "name": "BaseImages",
  "isPublic":"false",
  "mixins": {"FilesHandler":true},
  "http": {
    "path": "BaseImages"
  },
  "options": {
    "idInjection": false,
    "mysql": {"table": "Images"},
    "relations": {
      "customUser": {
        "model": "CustomUser",
        "type": "hasMany",
        "foreignKey": "mainImageId"
      }
    }
  },
  "properties": {
    "id": {
      "type": "number",
      "generated": true
    },
    "title": "string",
    "created": "date",
    "modified": "date",
    "category": "string",
    "description": "string",
    "format": "string",
    "owner": "number"
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ]
}
