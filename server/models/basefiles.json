{
  "name": "BaseFiles",
  "isPublic": false,
  "mixins": {
    "FilesHandler": true
  },
  "http": {
    "path": "BaseFiles"
  },
  "pathLink": "BaseFiles",
  "listViewWidget": "FileDisplayer",
  "options": {
    "idInjection": false,
    "mysql": {
      "table": "Files"
    }
  },
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "CustomUser",
      "foreignKey": "owner"
    }
  },
  "properties": {
    "id": {
      "type": "number",
      "generated": true
    },
    "title": "string",
    "created": "date",
    "modified": "date",
    "description": "string",
    "format": "string",
    "category": "string",
    "owner": "number"
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ]
}